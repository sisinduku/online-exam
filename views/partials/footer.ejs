<script>
  $("#<%= page %>").addClass("active");
</script>
<%
  if(hasOwnProperty('message')){
  if(message == 'success'){
%>
  <script>
    swal({
      title: 'Success!',
      text: 'Success Update Data',
      type: 'success',
      confirmButtonText: 'OK'
    })
  </script>
  <%
  }
  else if(message.toLowerCase().indexOf('error')> -1){
    %>
    <script>
      swal({
        title: 'Error!',
        text: 'Oopss, Something wrong :(',
        type: 'error',
        confirmButtonText: 'OK'
      })
    </script>
    <%
  }
  else if(message == 'logoutsuccess') {
    %>
    <script>
      swal({
        title: 'Success!',
        text: 'Success Logout',
        type: 'success',
        confirmButtonText: 'OK'
      })
    </script>
    <%
  }
  else if(message =='loginsuccess'){
    %>
    <script>
      swal({
        title: 'Success!',
        text: 'Success Login Bung',
        type: 'success',
        confirmButtonText: 'OK'
      })
    </script>
  <%
  }
}
else{

}
%>
